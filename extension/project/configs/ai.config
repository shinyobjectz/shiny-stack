# AI Configuration
# This file contains settings for AI-related features

[ai]
# AI model settings
model = "gpt-4"
temperature = 0.7
max_tokens = 2048

# API configuration
api_key = "your-api-key-here"
endpoint = "https://api.openai.com/v1"

[ai.features]
# Feature flags
code_completion = true
code_review = true
documentation_generation = false

[ai.prompts]
# Custom prompts
system_prompt = "You are a helpful AI assistant for coding tasks."
code_review_prompt = "Please review this code for potential issues and improvements."

[ai.limits]
# Rate limiting
requests_per_minute = 60
max_concurrent_requests = 5 